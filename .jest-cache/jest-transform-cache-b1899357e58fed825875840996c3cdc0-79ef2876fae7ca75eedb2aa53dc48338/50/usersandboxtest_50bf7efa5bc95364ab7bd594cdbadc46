        });
        (0, bun_test_1.it)('should handle validatePath with multiple trailing slashes', () => {
            const userRoot = path.resolve(TEST_USER_DATA_PATH, '1') + path.sep + path.sep;
            const result = user_sandbox_1.UserSandbox.validatePath('1', userRoot);
            (0, bun_test_1.expect)(result).toBe(true);
        });
    });
});
