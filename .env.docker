# Database (PostgreSQL)
POSTGRES_DB=openmake
POSTGRES_USER=openmake
POSTGRES_PASSWORD=openmake_secret_2026
DB_PORT=5432

# AI Service (Python FastAPI)
AI_SERVICE_PORT=8000
OLLAMA_BASE_URL=http://host.docker.internal:11434
OLLAMA_DEFAULT_MODEL=gemini-3-flash-preview:cloud
OLLAMA_KOREAN_MODEL=gemini-3-flash-preview:cloud
OLLAMA_TIMEOUT=120000

# Gemini Settings
GEMINI_THINK_ENABLED=true
GEMINI_THINK_LEVEL=high
GEMINI_NUM_CTX=32768
GEMINI_EMBEDDING_MODEL=gemini-3-flash-preview:cloud
GEMINI_WEB_SEARCH_ENABLED=true

# API Gateway (Go)
API_PORT=52416

# Frontend (Next.js)
FRONTEND_PORT=3000
NEXT_PUBLIC_API_URL=http://localhost:52416
NEXT_PUBLIC_WS_URL=ws://localhost:52416

# Auth
JWT_SECRET=openmake_jwt_secret_2026

# Google APIs
GOOGLE_API_KEY=
GOOGLE_CSE_ID=

# Logging
LOG_LEVEL=info
